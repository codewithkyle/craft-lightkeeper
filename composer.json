{
    "name": "codewithkyle/lightkeeper",
    "description": "Continuous performance monitoring for your Craft CMS websites.",
    "type": "craft-plugin",
    "version": "1.3.3",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "lighthouse",
        "performance",
        "user-experience",
        "reporting",
        "auditing"
    ],
    "support": {
        "docs": "https://github.com/codewithkyle/craft-lightkeeper/blob/master/README.md",
        "issues": "https://github.com/codewithkyle/craft-lightkeeper/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kyle Andrews",
            "homepage": "https://kyleandrews.dev/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "php": "^7.2|^8.0"
    },
    "autoload": {
        "psr-4": {
          "codewithkyle\\lightkeeper\\": "src/"
        }
    },
    "extra": {
        "name": "Lightkeeper",
        "handle": "lightkeeper",
        "developer": "Kyle Andrews",
        "developerUrl": "https://kyleandrews.dev/",
        "documentationUrl": "https://github.com/codewithkyle/craft-lightkeeper/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/codewithkyle/craft-lightkeeper/master/CHANGELOG.md",
        "components": {
            "lightkeeperService": "codewithkyle\\lightkeeper\\services\\LightkeeperService"
        },
        "class": "codewithkyle\\lightkeeper\\Lightkeeper"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
